	title = {Life prediction model for grid-connected {Li}-ion battery energy storage system},
	doi = {10.23919/ACC.2017.7963578},
	abstract = {Lithium-ion (Li-ion) batteries are being deployed on the electrical grid for a variety of purposes, such as to smooth fluctuations in solar renewable power generation. The lifetime of these batteries will vary depending on their thermal environment and how they are charged and discharged. To optimal utilization of a battery over its lifetime requires characterization of its performance degradation under different storage and cycling conditions. Aging tests were conducted on commercial graphite/nickel-manganese-cobalt (NMC) Li-ion cells. A general lifetime prognostic model framework is applied to model changes in capacity and resistance as the battery degrades. Across 9 aging test conditions from 0°C to 55°C, the model predicts capacity fade with 1.4\% RMS error and resistance growth with 15\% RMS error. The model, recast in state variable form with 8 states representing separate fade mechanisms, is used to extrapolate lifetime for example applications of the energy storage system integrated with renewable photovoltaic (PV) power generation. Uncertainty quantification and further validation are needed.},
	booktitle = {2017 {American} {Control} {Conference} ({ACC})},
	author = {Smith, K. and Saxon, A. and Keyser, M. and Lundstrom, B. and {Ziwei Cao} and Roc, A.},
	month = may,
	year = {2017},
	note = {ISSN: 2378-5861},
	keywords = {Aging, Batteries, Data models, Electrodes, Mathematical model, NiMnCoLi, PV power generation, RMS error, US Department of Defense, ageing, aging tests, battery storage plants, cobalt compounds, commercial graphite-nickel-manganese-cobalt lithium-ion cells, cycling conditions, electrical grid, fade mechanisms, general lifetime prognostic model framework, graphite, grid-connected lithium-ion battery energy storage system, life prediction model, lithium compounds, manganese compounds, modeling, nickel compounds, performance degradation characterization, power grids, renewable photovoltaic power generation, secondary cells, solar renewable power generation, temperature 0 degC to 55 degC, thermal environment, uncertainty quantification},
	pages = {4062--4068},
}

