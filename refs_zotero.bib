@article{aiken_accelerated_2020,
	title = {Accelerated {Failure} in {Li}[{Ni0}.{5Mn0}.{3Co0}.2]{O2}/{Graphite} {Pouch} {Cells} {Due} to {Low} {LiPF6} {Concentration} and {Extended} {Time} at {High} {Voltage}},
