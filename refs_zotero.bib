@article{liu_effects_2020,
	title = {Effects of {Current} {Density} on {Defect}-{Induced} {Capacity} {Fade} through {Localized} {Plating} in {Lithium}-{Ion} {Batteries}},
	volume = {167},
	issn = {1945-7111},
	url = {https://doi.org/10.1149%2F1945-7111%2Fabb838},
	doi = {10.1149/1945-7111/abb838},
	abstract = {Because of material heterogeneity and non-uniform ion transport, lithium plating, the formation of metallic lithium, can sometimes occur only in specific locations. Such localized plating can significantly reduce battery capacity through locally accelerated aging and also imposes safety concerns. In this work, we investigate the effects of current density on localized plating through experimental studies and numerical simulations, and demonstrate the plating results using LCO/graphite coin cells. As the current density increases, the critical size that triggers non-uniform plating decreases, and we directly correlate size and current density to capacity fade of a battery. We also identify transitions from no plating, to heterogeneous plating, and to homogeneous plating. We show that diffusion plays a critical role in the aggregation of lithium, and the critical size is negatively correlated with current density and the effective diffusion length. This work can provide insights into the quick charging of batteries that are subject to material defects or non-uniform current distributions.},
	language = {en},
	number = {13},
	urldate = {2020-09-28},
	journal = {Journal of The Electrochemical Society},
	author = {Liu, Xinyi M. and Arnold, Craig B.},
	month = sep,
	year = {2020},
	note = {Publisher: The Electrochemical Society},
	pages = {130519},
}

